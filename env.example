# GCP Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1

# Gemini API
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-2.0-flash-exp

# Cloud Storage
GCS_RAW_BUCKET=doc-processing-raw
GCS_PROCESSED_BUCKET=doc-processing-processed

# BigQuery
BIGQUERY_DATASET=document_processing
BIGQUERY_LOCATION=US

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=production
API_HOST=0.0.0.0
API_PORT=8080

# Confidence Thresholds
LOW_CONFIDENCE_THRESHOLD=0.7
REVIEW_REQUIRED_THRESHOLD=0.5

# Cloud Run and Cloud Functions
CLOUD_RUN_SERVICE=doc-processor
CLOUD_FUNCTION_NAME=document-trigger
PROCESSING_SERVICE_URL=https://doc-processor-xxxxx.run.app
